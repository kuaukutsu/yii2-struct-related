{
    "name": "kuaukutsu/yii2-struct-related",
    "description": "Save the relationship between objects",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",

    "authors": [
        {
            "name": "Dmitriy Krivopalov",
            "email": "dimarik82@gmail.com"
        }
    ],

    "support": {
        "issues": "https://github.com/kuaukutsu/yii2-struct-related/issues",
        "source": "https://github.com/kuaukutsu/yii2-struct-related"
    },

    "require": {
        "php": ">=7.2.0",
        "yiisoft/yii2": "~2.0.14"
    },

    "require-dev": {
